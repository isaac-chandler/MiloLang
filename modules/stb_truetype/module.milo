stbtt_GetGlyphHMetrics :: (info: *stbtt_fontinfo, glyph_index: s32, advanceWidth: *s32, leftSideBearing: *s32) #external "stb_truetype"
stbtt_GetGlyphKernAdvance :: (info: *stbtt_fontinfo, glyph1: s32, glyph2: s32) -> s32 #external "stb_truetype"
stbtt_GetGlyphBox :: (info: *stbtt_fontinfo, glyph_index: s32, x0: *s32, y0: *s32, x1: *s32, y1: *s32) -> s32 #external "stb_truetype"
